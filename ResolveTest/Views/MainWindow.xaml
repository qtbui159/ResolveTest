<Window x:Class="ResolveTest.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ResolveTest.Views"
        xmlns:prism="http://prismlibrary.com/"
        mc:Ignorable="d" prism:ViewModelLocator.AutoWireViewModel="True"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBlock Text="Expect Result:"/>
        <TextBlock Text="{Binding NewObj}" Margin="0,40,0,0"/>

        <TextBlock Text="Fact Result:"  Margin="0,80,0,0"/>
        <TextBlock Text="{Binding ResolveObj}"  Margin="0,120,0,0"/>
    </Grid>
</Window>
